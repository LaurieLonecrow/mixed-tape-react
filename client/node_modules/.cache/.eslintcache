 from \"../Playlist\";\n\n//styles\nimport { Wrapper, Content, Chat} from './Form.styles'\n\n\nconst Form = (props) => {\n    //modal functionality of form \n    const [formView, setFormView] = useState(false)\n    \n    function showForm(e){\n        setFormView(true)\n    }\n    \n    //handling text inputs\n    const[input, setInput]=useState({\n        trackId: \"\",\n        content: \"\"\n    });\n\n    function handleChange(e){\n        const value = e.target.value\n        const spotifyId = props.trackId\n        setInput({\n            content: value,\n            trackId: spotifyId\n        });\n    }\n\n    function handleSubmit(e){\n        e.preventDefault();\n        props.onSubmit(input);\n        setInput({trackId:'', content:''});\n        setFormView(false);\n    }\n\nreturn (\n    <Wrapper>\n        <Chat onClick={showForm}/>\n        {formView ?\n        (<Content>\n        <form className='form-container'onSubmit={handleSubmit}>\n            <textarea type='textarea' name='content' rows=\"7\" cols=\"100\"\n                placeholder='Insert a description...' value={input.content} onChange={handleChange}/>\n            <button type='submit' disabled={!input.content}>Create</button>\n        </form>\n        </Content> ) : null}\n    </Wrapper>\n    \n)\n};\n\nexport default Form","/Users/admin/Documents/Web Development/MixedTape/mixed-tape-react/mixed-tape-react/client/src/components/Loader/Loader.js",[],"/Users/admin/Documents/Web Development/MixedTape/mixed-tape-react/mixed-tape-react/client/src/components/Loader/Loader.styles.js",[],"/Users/admin/Documents/Web Development/MixedTape/mixed-tape-react/mixed-tape-react/client/src/components/Text/Text.js",[],"/Users/admin/Documents/Web Development/MixedTape/mixed-tape-react/mixed-tape-react/client/src/components/Text/Text.styles.js",[],"/Users/admin/Documents/Web Development/MixedTape/mixed-tape-react/mixed-tape-react/client/src/components/Tracks/Tracks.js",["125"],"/Users/admin/Documents/Web Deve